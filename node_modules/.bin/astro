#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Proyectos_astro/Performarce/Web/node_modules/.pnpm/astro@4.5.13_@types+node@20.12.3_typescript@5.4.3/node_modules/astro/node_modules:/mnt/d/Proyectos_astro/Performarce/Web/node_modules/.pnpm/astro@4.5.13_@types+node@20.12.3_typescript@5.4.3/node_modules:/mnt/d/Proyectos_astro/Performarce/Web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Proyectos_astro/Performarce/Web/node_modules/.pnpm/astro@4.5.13_@types+node@20.12.3_typescript@5.4.3/node_modules/astro/node_modules:/mnt/d/Proyectos_astro/Performarce/Web/node_modules/.pnpm/astro@4.5.13_@types+node@20.12.3_typescript@5.4.3/node_modules:/mnt/d/Proyectos_astro/Performarce/Web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
